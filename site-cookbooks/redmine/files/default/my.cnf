[client]
port = 3306
socket = /var/lib/mysql/mysql.sock

[mysqld_safe]
socket = /var/lib/mysql/mysql.sock

[mysqld]
user=mysql
pid-file = /var/lib/mysql/mysql.pid
socket = /var/lib/mysql/mysql.sock
port = 3306
datadir = /var/lib/mysql
character-set-server=utf8

[mysql]
default-character-set=utf8
!includedir /etc/mysql/conf.d
